<%= form_for :projects, url: projects_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>

  <p>
    <%= link_to 'Back', projects_path, class: "btn btn-default" %>
  </p>

<% end %>