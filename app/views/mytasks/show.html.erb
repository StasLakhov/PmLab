<h3>My Tasks</h3>

<table class="table table-striped">
  <thead>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Status</th>
    <th>Cteated</th>
    <th>Finished</th>
    <th colspan="3"></th>
  </tr>
  </thead>

      <tr>
        <td><%= @mytask.taskname %></td>
        <td><%= @mytask.taskcontent %></td>
        <td><%= @mytask.taskstatus %></td>
        <td><%= @mytask.createtask %></td>
        <td><%= @mytask.endtask %></td>
        <td><%= link_to '', edit_mytask_path(@mytask), class: "glyphicon glyphicon-edit icon-edit" %></td>
        <td><%= link_to '', @mytask,
                        method: :delete,
                        data: { confirm: 'Are you sure?' },
                        class: "glyphicon glyphicon-trash" %>
        </td>
      </tr>

</table>

<p>
  <%= link_to 'Back', mytasks_path, class: "btn btn-default" %>
</p>

